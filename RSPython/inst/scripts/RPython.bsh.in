R_HOME=@R_HOME@
export R_HOME
 
if test -z "${PYTHONPATH}" ; then
  PYTHONPATH=@R_PACKAGE_DIR@/libs:@R_PACKAGE_DIR@/Python
else
  PYTHONPATH=${PYTHONPATH}:@R_PACKAGE_DIR@/libs:@R_PACKAGE_DIR@/Python
fi

export PYTHONPATH
 
if test -z "${LD_LIBRARY_PATH}" ; then
  LD_LIBRARY_PATH=@R_SHARED_LIB_DIR@
else
  LD_LIBRARY_PATH=${LD_LIBRARY_PATH}:@R_SHARED_LIB_DIR@
fi

export LD_LIBRARY_PATH