setenv R_HOME @R_HOME@

if !(${?PYTHONPATH}) then
  setenv PYTHONPATH @R_PACKAGE_DIR@/libs:@R_PACKAGE_DIR@/Python
else
  setenv PYTHONPATH ${PYTHONPATH}:@R_PACKAGE_DIR@/libs:@R_PACKAGE_DIR@/Python
endif

if !(${?LD_LIBRARY_PATH}) then
  setenv LD_LIBRARY_PATH @R_SHARED_LIB_DIR@
else
  setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:@R_SHARED_LIB_DIR@
endif
